{
  "extends": "airbnb-base",
  "rules": {
    "no-underscore-dangle": ["error", { "allow": ["_id"] }],
    "no-unused-vars": "off",
    "no-shadow": "off",
    "quotes": ["error", "double"],
    "consistent-return": "off",
    "func-names": "off",
    "implicit-arrow-linebreak": "off",
    "function-paren-newline": "off",
    "comma-dangle": "off",
    "operator-linebreak": "off",
    "import/no-extraneous-dependencies": "off"
  }
}
